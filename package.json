{
  "name": "productmanager",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test:integration": "NODE_ENV=test jest tests/integration --runInBand",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "NODE_ENV=test prisma migrate deploy",
    "db:reset:test": "NODE_ENV=test prisma migrate reset --force",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "studio:dev": "npx prisma studio",
    "studio:prod": "dotenv -e .env.prod -- npx prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
